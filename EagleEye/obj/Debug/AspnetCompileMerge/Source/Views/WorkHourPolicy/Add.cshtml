    @* Events *@
    <script type="text/javascript">

        var chk_mon;
        var chk_tue;
        var chk_wed;
        var chk_thu;
        var chk_fri;
        var chk_sat;
        var chk_sun;
        var workHourArr = [];
        

        $("#btnSave").click(function (e) {

            e.preventDefault();
            var workHourPolicy = {
                "Code":0,
                "PolicyName": $("#workHourPolicy").val(),
                "isActive": true,
            };

            //getting values from model
            gettingValue();
       
            var emptyFieldCount = 0;
            var fullFieldCount = 0;

            if (workHourArr.length > 0) {
                for (let i = 0; i < workHourArr.length; i++) {
             
                    if (workHourArr[i]["Workhour"] == "") {
                        ++emptyFieldCount;
                        switch (workHourArr[i].Day) {
                            case 1:
                                showWarningNoti("Please Add WorkHour on Monday");
                                break;
                            case 2:
                                showWarningNoti("Please Add WorkHour on Tuesday");
                                break;
                            case 3:
                                showWarningNoti("Please Add WorkHour on Wednesday");
                                break;
                            case 4:
                                showWarningNoti("Please Add WorkHour on Thursday");
                                break;
                            case 5:
                                showWarningNoti("Please Add WorkHour on Friday");
                                break;
                            case 6:
                                showWarningNoti("Please Add WorkHour on Saturday");
                                break;
                            case 7:
                                showWarningNoti("Please Add WorkHour on Sunday");
                                break;

                        }

                    }
                    else {
                        ++fullFieldCount;

                    }
                }

                if (fullFieldCount > 0) {
                    if (workHourPolicy.PolicyName != "") {

                        let flag = validityCheckFor24Hours(workHourArr);
                        if (!flag) {
                            $.ajax({

                                url: '@Url.Action("AddWorkHour", "WorkHourPolicy")',
                                type: "POST",
                                dataType: "json",
                                cache: false,
                                data: { workHour: workHourPolicy, workHourDetail: workHourArr },
                                success: function (data) {
                                    if (data.result) {
                                        showSuccessNoti("WorkHour Added Successfully!");
                                        setTimeout(function () {
                                            window.location.href = '@Url.Action("Index", "WorkHourPolicy")';
                                        }, 1000);
                                    }
                                    else {
                                        showWarningNoti("Please Check Policy to Proceed...");
                                    }

                                }
                            });
                        }
                    } else {

                        showWarningNoti("Policy Name must be Required...");
                    }
                }
                

            } else {
                showWarningNoti("Select atleast one to Proceed...");
            }




        });



        //For monday
        $("#chk_1").click(function (e) {
             chk_mon = $(this).is(":checked") == true ? showingFields('1') : hidingFields('1');

        });

        $("#chk_overtime_1").click(function (e) {
            ck_ovt_Enabling_Disabling('1');
        });

        //For tuesday
        $("#chk_2").click(function (e) {
             chk_tue = $(this).is(":checked") == true ? showingFields('2') : hidingFields('2');

        });

        $("#chk_overtime_2").click(function (e) {
            ck_ovt_Enabling_Disabling('2');
        });

        //For wednesday
        $("#chk_3").click(function (e) {
             chk_wed = $(this).is(":checked") == true ? showingFields('3') : hidingFields('3');

        });

        $("#chk_overtime_3").click(function (e) {
            ck_ovt_Enabling_Disabling('3');
        });

        //For thursday
        $("#chk_4").click(function (e) {
