        });

        $("#chk_overtime_6").click(function (e) {
            ck_ovt_Enabling_Disabling('6');
        });

        //For sunday
        $("#chk_7").click(function (e) {
             chk_sun = $(this).is(":checked") == true ? showingFields('7') : hidingFields('7');

        });

        $("#chk_overtime_7").click(function (e) {
            ck_ovt_Enabling_Disabling('7');
        });
    </script>




    @* Methods *@
    <script type="text/javascript">

        function forMasking() {
            for (let i = 1; i <= 7; i++) {

                vanillaTextMask.maskInput({
                    inputElement: $(`#time_workHour_${i}`)[0],
                    mask: [/[0-9]/, /\d/, ':', /\d/, /\d/]
                });
                vanillaTextMask.maskInput({
                    inputElement: $(`#time_overtime_${i}`)[0],
                    mask: [/[0-9]/, /\d/, ':', /\d/, /\d/]
                });
                vanillaTextMask.maskInput({
                    inputElement: $(`#time_breakHour_${i}`)[0],
                    mask: [/[0-9]/, /\d/, ':', /\d/, /\d/]
                });
            }
        }

        function gettingValue() {
            workHourArr = [];
            for (let i = 1; i <= 7; i++) {
                var model = {};
                if ($(`#chk_${i}`).is(":checked") == true) {
                    model.DayCheck = $(`#chk_${i}`).is(":checked");
                    model.Day = i;
                    model.Workhour = $(`#time_workHour_${i}`).val();
                    model.Overtime = $(`#time_overtime_${i}`).val();
                    model.Breakhour = $(`#time_breakHour_${i}`).val();
                    model.isOvertimeActive = $(`#chk_overtime_${i}`).is(":checked");
                    workHourArr.push(model);
                }

            }

        }

        function ck_ovt_Enabling_Disabling(param) {

            let chckOverTime = `#chk_overtime_${param}`;
            let overTime = `#time_overtime_${param}`;
            $(chckOverTime).is(":checked") == true ? $(overTime).attr('disabled', false) : $(overTime).attr('disabled', true);

        }

        function hidingFields(param) {

            let workHour = `#time_workHour_${param}`;
            let overTime = `#time_overtime_${param}`;
            let chckOverTime = `#chk_overtime_${param}`;
            let breakHours = `#time_breakHour_${param}`;
            $(workHour).attr('disabled', true);
            $(overTime).attr('disabled', true);
            $(breakHours).attr('disabled', true);
            $(chckOverTime).attr('disabled', true);
        }

        function showingFields(param) {

            let workHour = `#time_workHour_${param}`;
            let overTime = `#time_overtime_${param}`;
            let chckOverTime = `#chk_overtime_${param}`;
            let breakHours = `#time_breakHour_${param}`;
            $(workHour).attr('disabled', false);
            //$(overTime).attr('disabled', false);
            $(breakHours).attr('disabled', false);
            $(chckOverTime).attr('disabled', false);
        }

        function disableDays_checkbox(param) {

            let check = `#chk_${param}`
            $(check).attr('disabled', true);

        }

        function enableDays_checkbox(param) {

            let check = `#chk_${param}`
            $(check).attr('disabled', false);

        }
    </script>


    <script>

        function validityCheckFor24Hours(model) {
          console.log(model)
            let day;
            let WH;
            let OT;
            let BH;
            let total;
            let flag = false;
            for (let i = 0; i < model.length; i++) {
                switch (model[i].Day) {

                    case 1:
                       
                        day = "Monday";
                        WH = model[i].Workhour == "" ? 0 : parseInt(model[i].Workhour);
                        if (model[i].isOvertimeActive) {
                            OT = model[i].Overtime == "" ? 0 : parseInt(model[i].Overtime);
                        } else {
